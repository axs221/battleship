{
  parser: "babel-eslint",
  extends: [
    "airbnb",
    "plugin:flowtype/recommended",
  ],
  plugins: [
    "react",
    "flowtype",
  ],
  rules: {
    linebreak-style: "off",
    max-len: "off",
    class-methods-use-this: "off",
    jsx-a11y/no-static-element-interactions: "warn",
    jsx-a11y/no-noninteractive-element-interactions: "warn",
  },
  env: {
    browser: true,
    jquery: true,
  },
}
