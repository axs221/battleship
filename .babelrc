{
  "compact": "true",
  "presets": [
    "es2015",
    "react",
    "stage-3"
  ],
  "plugins": [
    "transform-flow-strip-types",
    "transform-class-properties",
    "transform-es2015-block-scoping",
    ["transform-es2015-classes", { loose: true }],
    "transform-proto-to-assign",
    "react-hot-loader/babel"
  ]
}
